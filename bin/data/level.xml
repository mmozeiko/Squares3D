<xml>
  <materials>
    <material id="grass">
      <texture>
        <path>grass.tga</path>
      </texture>
      <colors>
        <ambient r="0.8" g="0.8" b="0.8" />
      </colors>
    </material>

    <material id="football">
      <texture>
        <path>ball.tga</path>
        <bump_path>ball_bump.tga</bump_path>
      </texture>
      <shader name="bump" />
      <colors>
        <ambient r="0.8" g="0.8" b="0.8" />
      </colors>
    </material>

    <material id="lady">
      <texture>
        <path>lady.tga</path>
      </texture>
      <colors>
        <ambient r="0.8" g="0.8" b="0.8" />
      </colors>
    </material>

    <material id="boob">
      <texture>
        <path>boob.tga</path>
      </texture>
      <colors>
        <ambient r="0.8" g="0.8" b="0.8" />
      </colors>
    </material>

    <material id="pavement">
      <texture>
        <path>pavement.tga</path>
      </texture>
      <colors>
        <ambient r="0.8" g="0.8" b="0.8" />
      </colors>
    </material>

    <material id="brick">
      <texture>
        <path>brick.tga</path>
      </texture>
      <colors>
        <ambient r="0.5" g="0.5" b="0.5" />
        <!--specular r="1.0" g="1.0" b="1.0" /-->
        <!--emission r="0.0" g="0.0" b="0.0" /-->
        <shine>1.0</shine>
      </colors>
      <!--shader name="bump" texture="brick_bump.tga" /-->
    </material>

    <material id="wood">
      <texture>
        <path>wood.tga</path>
      </texture>
      <colors>
        <ambient r="0.5" g="0.5" b="0.5" />
        <!--specular r="1.0" g="1.0" b="1.0" /-->
        <!--emission r="0.0" g="0.0" b="0.0" /-->
        <shine>1.0</shine>
      </colors>
    </material>

    <material id="box">
      <texture>
        <path>box.tga</path>
      </texture>
      <colors>
        <ambient r="0.5" g="0.5" b="0.5" />
        <!--specular r="1.0" g="1.0" b="1.0" /-->
        <!--emission r="0.0" g="0.0" b="0.0" /-->
        <shine>1.0</shine>
      </colors>
    </material>

    <properties material1="brick" material2="human" sF="1.0" kF="0.0" />
  </materials>
  
  <collisions>
    <collision id="box1" type="box" mass="0.2" material="box">
      <size x="0.50" y="0.20" z="0.15" />
    </collision>

    <collision id="football" type="sphere" mass="0.4" material="football">
      <radius x="0.11" y="0.11" z="0.11" />
    </collision>
    
    <collision id="bumbaLiela" type="sphere" mass="100.0" material="box">
      <radius x="2" y="2" z="3" />
    </collision>
    
    <collision id="box2" type="box" mass="500.0" material="box">
      <size x="1.5" y="1.0" z="1.5" />
      <!--offset x="2.0" y="0.0" z="2.0" />
        <rotation x="0.0" y="0.0" z="0.0" /-->
    </collision>

    <collision id="thinBox" type="box" mass="50.0" material="box">
      <size x="0.1" y="1.0" z="0.1" />
      <offset x="0.0" y="1.0" z="0.0" />
      <!--rotation x="0.0" y="0.0" z="0.0" /-->
    </collision>

    <collision id="heavyBox" type="sphere" mass="10000.0" material="box">
      <radius x="0.2" y="0.1" z="0.2" />
      <offset x="0.0" y="1.55" z="0.0" />
      <!--rotation x="0.0" y="0.0" z="0.0" /-->
    </collision>

    <collision id="body" type="sphere" mass="10.0" material="box">
      <radius x="0.3" y="1.2" z="0.5" />
      <offset x="0.0" y="0.0" z="0.0" />
      <!--rotation x="0.0" y="0.0" z="0.0" /-->
    </collision>
    <collision id="head" type="sphere" mass="10.0" material="lady">
      <radius x="0.3" y="0.4" z="0.3" />
      <offset x="0.0" y="1.2" z="0.0" />
    </collision>
    <collision id="boob1" type="sphere" mass="10.0" material="boob">
      <radius x="0.25" y="0.2" z="0.2" />
      <offset x="0.2" y="0.6" z="0.2" />
      <rotation x="0.0" y="1.7" z="1.7" />
    </collision>
    <collision id="boob2" type="sphere" mass="10.0" material="boob">
      <radius x="0.25" y="0.2" z="0.2" />
      <offset x="0.2" y="0.6" z="-0.2" />
      <rotation x="0.0" y="1.7" z="1.7" />
    </collision>
    <collision id="hand1" type="box" mass="10.0" material="box">
      <size x="0.15" y="1.0" z="0.1" />
      <offset x="0.0" y="0.4" z="-0.5" />
      <rotation x="0.5" y="0.0" z="0.0" />
    </collision>
    <collision id="hand2" type="box" mass="10.0">
      <size x="0.15" y="1.0" z="0.1" />
      <offset x="0.0" y="0.4" z="0.5" />
      <rotation x="-0.5" y="0.0" z="0.0" />
    </collision>
    <collision id="leg" type="box" mass="100.0">
      <size x="0.8" y="0.05" z="0.8" />
      <offset x="0.0" y="-1.2" z="0.0" />
    </collision>

    <collision id="tree1" type="tree">
      <face material="pavement">
        <vertex x="-3.0" y="0.0" z="3.0" u="0.0" v="0.0" />
        <vertex x="0.0"  y="0.0" z="3.0" u="1.0" v="0.0" />
        <vertex x="0.0"  y="0.0" z="0.0" u="1.0" v="1.0" />
        <vertex x="-3.0" y="0.0" z="0.0" u="0.0" v="1.0" />
      </face>

      <face material="pavement">
        <vertex x="0.0" y="0.0" z="3.0" u="0.0" v="0.0" />
        <vertex x="3.0" y="0.0" z="3.0" u="1.0" v="0.0" />
        <vertex x="3.0" y="0.0" z="0.0" u="1.0" v="1.0" />
        <vertex x="0.0" y="0.0" z="0.0" u="0.0" v="1.0" />
      </face>

      <face material="pavement">
        <vertex x="0.0" y="0.0" z="0.0"  u="0.0" v="0.0" />
        <vertex x="3.0" y="0.0" z="0.0"  u="1.0" v="0.0" />
        <vertex x="3.0" y="0.0" z="-3.0" u="1.0" v="1.0" />
        <vertex x="0.0" y="0.0" z="-3.0" u="0.0" v="1.0" />
      </face>

      <face material="pavement">
        <vertex x="-3.0" y="0.0" z="0.0"  u="0.0" v="0.0" />
        <vertex x="0.0"  y="0.0" z="0.0"  u="1.0" v="0.0" />
        <vertex x="0.0"  y="0.0" z="-3.0" u="1.0" v="1.0" />
        <vertex x="-3.0" y="0.0" z="-3.0" u="0.0" v="1.0" />
      </face>

      <face material="grass">
        <vertex x="-10.0" y="0.0" z="10.0" u="0.0"  v="0.0" />
        <vertex x="10.0"  y="0.0" z="10.0" u="10.0" v="0.0" />
        <vertex x="10.0"  y="0.0" z="3.0"  u="10.0" v="3.5" />
        <vertex x="-10.0" y="0.0" z="3.0"  u="0.0"  v="3.5" />
      </face>

      <face material="grass">
        <vertex x="3.0"  y="0.0" z="3.0"  u="0.0" v="0.0" />
        <vertex x="10.0" y="0.0" z="3.0"  u="3.5" v="0.0" />
        <vertex x="10.0" y="0.0" z="-3.0" u="3.5" v="3.0" />
        <vertex x="3.0"  y="0.0" z="-3.0" u="0.0" v="3.0" />
      </face>

      <face material="grass">
        <vertex x="-10.0" y="0.0" z="-3.0"  u="0.0"  v="0.0"  />
        <vertex x="10.0"  y="0.0" z="-3.0"  u="10.0" v="0.0"  />
        <vertex x="10.0"  y="0.0" z="-10.0" u="10.0" v="3.5"  />
        <vertex x="-10.0" y="0.0" z="-10.0" u="0.0"  v="3.5"  />
      </face>

      <face material="grass">
        <vertex x="-10.0" y="0.0" z="3.0"  u="0.0" v="0.0" />
        <vertex x="-3.0"  y="0.0" z="3.0"  u="3.5" v="0.0" />
        <vertex x="-3.0"  y="0.0" z="-3.0" u="3.5" v="3.0" />
        <vertex x="-10.0" y="0.0" z="-3.0" u="0.0" v="3.0" />
      </face>

      <face material="brick">
        <vertex x="-10.0" y="3.0" z="10.0" u="0.0"  v="0.0" />
        <vertex x="10.0"  y="3.0" z="10.0" u="10.0" v="0.0" />
        <vertex x="10.0"  y="0.0" z="10.0" u="10.0" v="1.5" />
        <vertex x="-10.0" y="0.0" z="10.0" u="0.0"  v="1.5" />
      </face>

      <face material="brick">
        <vertex x="10.0" y="3.0" z="10.0"  u="0.0"  v="0.0" />
        <vertex x="10.0" y="3.0" z="-10.0" u="10.0" v="0.0" />
        <vertex x="10.0" y="0.0" z="-10.0" u="10.0" v="1.5" />
        <vertex x="10.0" y="0.0" z="10.0"  u="0.0"  v="1.5" />
      </face>

      <face material="brick">
        <vertex x="-10.0" y="3.0" z="-10.0" u="0.0"  v="0.0" />
        <vertex x="-10.0" y="0.0" z="-10.0" u="0.0"  v="1.5" />
        <vertex x="10.0"  y="0.0" z="-10.0" u="10.0" v="1.5" />
        <vertex x="10.0"  y="3.0" z="-10.0" u="10.0" v="0.0" />
      </face>

      <face material="brick">
        <vertex x="-10.0" y="3.0" z="10.0"  u="0.0"  v="0.0" />
        <vertex x="-10.0" y="0.0" z="10.0"  u="0.0"  v="1.5" />
        <vertex x="-10.0" y="0.0" z="-10.0" u="10.0" v="1.5" />
        <vertex x="-10.0" y="3.0" z="-10.0" u="10.0" v="0.0" />
      </face>
    </collision>
  </collisions>


  <bodies>
    <body id="level">
      <position x="0.0" y="0.0" z="0.0" />
      <rotation x="0.0" y="0.0" z="0.0" />
      <collision>tree1</collision>
    </body>

    <body id="box">
      <position x="-4.0" y="0.5" z="2.0" />
      <rotation x="0.0" y="0.5" z="0.0" />
      <collision>box2</collision>
    </body>

    <body id="football">
      <position x="0.0" y="0.5" z="0.0" />
      <rotation x="0.0" y="0.0" z="0.0" />
      <collision>football</collision>
    </body>
    
    <!--body id="sphere">
      <position x="-8.0" y="5.5" z="2.0" />
      <rotation x="0.0" y="0.5" z="0.0" />
      <collision>bumbaLiela</collision>
    </body-->

    <!--body id="combinedbox">
      <position x="-2.0" y="1.55" z="-2.0" />
      <rotation x="0.0" y="0.5" z="4.6" />
      <collision>box2</collision>
      <collision>thinBox</collision>
      <collision>heavyBox</collision>
    </body-->

    <body id="lady">
      <position x="3.0" y="2.0" z="-3.0" />
      <rotation x="0.0" y="45.0" z="0.0" />
      <collision>body</collision>
      <collision>head</collision>
      <collision>hand1</collision>
      <collision>hand2</collision>
      <collision>boob1</collision>
      <collision>boob2</collision>
      <collision>leg</collision>
    </body>


  </bodies>


  <joints>

  </joints>
 
</xml>
