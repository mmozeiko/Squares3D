<xml>
  <materials>
    <material id="grass">
      <texture>
        <path>grass.tga</path>
      </texture>
      <colors>
        <ambient r="0.8" g="0.8" b="0.8" />
      </colors>
    </material>

    <material id="player">
      <texture>
        <path>player.tga</path>
      </texture>
      <colors>
        <ambient r="0.8" g="0.8" b="0.8" />
      </colors>
    </material>
    
    <material id="football">
      <texture>
        <path>ball.tga</path>
        <bump_path>ball_bump.tga</bump_path>
      </texture>
      <shader name="bump" />
      <colors>
        <ambient r="0.8" g="0.8" b="0.8" />
      </colors>
    </material>

    <material id="lady">
      <texture>
        <path>lady.tga</path>
      </texture>
      <colors>
        <ambient r="0.8" g="0.8" b="0.8" />
      </colors>
    </material>

    <material id="boob">
      <texture>
        <path>boob.tga</path>
      </texture>
      <colors>
        <ambient r="0.8" g="0.8" b="0.8" />
      </colors>
    </material>

    <material id="pavement">
      <texture>
        <path>pavement.tga</path>
      </texture>
      <colors>
        <ambient r="0.8" g="0.8" b="0.8" />
      </colors>
    </material>

    <material id="brick">
      <texture>
        <path>rock.tga</path>
        <bump_path>rock_bump.tga</bump_path>
      </texture>
      <shader name="bump" />
      <colors>
        <ambient r="0.5" g="0.5" b="0.5" />
        <!--specular r="1.0" g="1.0" b="1.0" /-->
        <!--emission r="0.0" g="0.0" b="0.0" /-->
        <shine>1.0</shine>
      </colors>
      <!--shader name="bump" texture="brick_bump.tga" /-->
    </material>

    <material id="wood">
      <texture>
        <path>wood.tga</path>
      </texture>
      <colors>
        <ambient r="0.5" g="0.5" b="0.5" />
        <!--specular r="1.0" g="1.0" b="1.0" /-->
        <!--emission r="0.0" g="0.0" b="0.0" /-->
        <shine>1.0</shine>
      </colors>
    </material>

    <material id="box">
      <texture>
        <path>box.tga</path>
      </texture>
      <colors>
        <ambient r="0.5" g="0.5" b="0.5" />
        <!--specular r="1.0" g="1.0" b="1.0" /-->
        <!--emission r="0.0" g="0.0" b="0.0" /-->
        <shine>1.0</shine>
      </colors>
    </material>

    <properties material1="player" material2="player" staticFriction="1.0" kineticFriction="0.0" elasticityCoeficient="0.5" softnessCoeficient="0.15"/>
  </materials>
</xml>
